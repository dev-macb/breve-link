{
    "name": "breve-link",
    "version": "1.1.0",
    "main": "index.js",
    "repository": "https://github.com/dev-macb/breve-link.git",
    "author": "Miguel Alves Cordeiro Braz <dev.macb@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --transpile-only ./src/index.ts",
        "test": "jest",
        "build": "tsc",
        "start": "node ./build/index.js",
        "knex:make:seed": "knex --knexfile ./src/config/knex/Ambiente.ts seed:make",
        "knex:make:migrate": "knex --knexfile ./src/config/knex/Ambiente.ts migrate:make",
        "knex:seed": "knex --knexfile ./src/config/knex/Ambiente.ts seed:run",
        "knex:migrate": "knex --knexfile ./src/config/knex/Ambiente.ts migrate:latest",
        "knex:rollback": "knex --knexfile ./src/config/knex/Ambiente.ts migrate:rollback",
        "knex:rollback-all": "knex --knexfile ./src/config/knex/Ambiente.ts migrate:rollback --all"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/express": "^5.0.3",
        "@types/inversify": "^2.0.33",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.5.0",
        "jest": "^30.1.3",
        "sqlite3": "^5.1.7",
        "ts-jest": "^29.4.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "bcrypt": "^6.0.0",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "http-status-codes": "^2.3.0",
        "inversify": "^7.10.0",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.1.0",
        "reflect-metadata": "^0.2.2",
        "yup": "^1.7.0"
    }
}
